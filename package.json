{
  "name": "express-typescript-starter",
  "version": "0.1.0",
  "description": "A starting point for Node.js express apps with TypeScript",
  "repository": {
    "type": "git",
    "url": "https://github.com/Microsoft/TypeScript-Node-Starter"
  },
  "author": "Bowden Kelly",
  "license": "MIT",
  "scripts": {
    "start": "npm run serve",
    "build": "npm run build-sass && npm run build-ts && npm run lint && npm run copy-static-assets",
    "serve": "node dist/server.js",
    "dev:watch:server": "nodemon --delay 2.5 dist/server.js",
    "dev:serve-debug": "nodemon --inspect dist/server.js",
    "dev:server": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run dev:watch-ts\" \"nodemon --delay 2.5 dist/server.js\"",
    "dev:cli": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run dev:watch-ts\" \"nodemon --delay 2.5 dist/cli worker:translate\"",
    "dev:watch-ts": "tsc -w",
    "dev:watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run dev:watch-ts\" \"npm run serve-debug\"",
    "test": "jest --forceExit --coverage --verbose",
    "watch-test": "npm run test -- --watchAll",
    "build-ts": "tsc",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "copy-static-assets": "ts-node copyStaticAssets.ts",
    "debug": "npm run build && npm run watch-debug"
  },
  "dependencies": {
    "@google-cloud/translate": "^4.1.3",
    "@types/commander": "^2.12.2",
    "async": "^3.0.1",
    "bcrypt-nodejs": "^0.0.3",
    "bluebird": "^3.5.5",
    "body-parser": "^1.19.0",
    "commander": "^3.0.0",
    "compression": "^1.7.4",
    "connect-mongo": "^3.0.0",
    "dotenv": "^8.0.0",
    "errorhandler": "^1.5.1",
    "exceljs": "^1.15.0",
    "express": "^4.17.1",
    "express-flash": "0.0.2",
    "express-session": "^1.16.2",
    "express-validator": "^6.0.1",
    "fbgraph": "^1.4.4",
    "lodash": "^4.17.13",
    "lusca": "^1.6.1",
    "node-dependency-injection": "^2.3.5",
    "node-schedule": "^1.3.2",
    "nodemailer": "^6.2.1",
    "passport": "^0.4.0",
    "passport-facebook": "^3.0.0",
    "passport-local": "^1.0.0",
    "pug": "^2.0.4",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "winston": "^2.4.2"
  },
  "devDependencies": {
    "@types/async": "^3.0.0",
    "@types/bcrypt-nodejs": "^0.0.30",
    "@types/bluebird": "^3.5.27",
    "@types/body-parser": "^1.17.0",
    "@types/chai": "^4.1.7",
    "@types/compression": "^0.0.36",
    "@types/connect-mongo": "^0.0.42",
    "@types/dotenv": "^6.1.1",
    "@types/errorhandler": "^0.0.32",
    "@types/express": "^4.17.0",
    "@types/express-flash": "0.0.0",
    "@types/express-session": "^1.15.13",
    "@types/jest": "^24.0.15",
    "@types/jquery": "^3.3.29",
    "@types/lodash": "^4.14.134",
    "@types/lusca": "^1.6.0",
    "@types/mongodb": "^3.1.28",
    "@types/morgan": "^1.7.35",
    "@types/node": "^12.0.10",
    "@types/nodemailer": "^6.2.0",
    "@types/passport": "^1.0.0",
    "@types/passport-facebook": "^2.1.9",
    "@types/passport-local": "^1.0.33",
    "@types/request": "^2.48.1",
    "@types/shelljs": "^0.8.5",
    "@types/supertest": "^2.0.7",
    "@types/winston": "^2.3.9",
    "@typescript-eslint/eslint-plugin": "^1.12.0",
    "@typescript-eslint/parser": "^1.12.0",
    "chai": "^4.2.0",
    "concurrently": "^4.1.0",
    "eslint": "^5.0.0",
    "jest": "^24.8.0",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.1",
    "shelljs": "^0.8.3",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.2"
  }
}
